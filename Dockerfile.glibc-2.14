FROM appplant/go-cli:glibc-2.14
MAINTAINER Sebastian Katzer "haertel@appplant.de"

RUN go get github.com/urfave/cli

RUN mkdir $GOPATH/src/github.com/appPlant
RUN mkdir $GOPATH/src/github.com/appPlant/alpinepass

ENV ORBIT_HOME $APP_HOME/bintest/orbit
ENV ALPINEPASS_HOME $GOPATH/src/github.com/appPlant/alpinepass
